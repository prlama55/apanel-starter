{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "date": {
      "type": "date"
    },
    "title": {
      "type": "string"
    },
    "content": {
      "type": "blocks"
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "articles"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "meta.seo"
    },
    "testtext": {
      "type": "string",
      "required": true,
      "regex": "^(?!.*fail).*"
    },
    "testrepeatablecomp": {
      "displayName": "testrepeatablecomp",
      "type": "component",
      "repeatable": true,
      "component": "product.testrepeatablecomp"
    },
    "testsinglecomp": {
      "displayName": "testsinglecomp",
      "type": "component",
      "repeatable": false,
      "component": "product.testsinglecomp"
    },
    "testdz": {
      "type": "dynamiczone",
      "components": ["product.testnewcomponentexistingcategory", "product.variations"]
    }
  }
}
